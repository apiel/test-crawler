{
  "name": "test-crawler",
  "version": "0.7.6",
  "bin": {
    "test-crawler": "./test-crawler"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/apiel/test-crawler"
  },
  "scripts": {
    "test-crawler": "test-crawler",
    "prepublishOnly": "cp ../../README.md . && yarn isomor:build && yarn serv:build && chmod +x test-crawler",
    "isomor:build": "isomor-transpiler",
    "isomor:build:dev": "WATCH=true isomor-transpiler",
    "serv:build": "rimraf ./dist-server && tsc -p tsconfig.server.json",
    "serv": "yarn serv:build && isomor-server",
    "serv:dev": "nodemon --watch 'src-isomor/**/server/**/*' -e ts,tsx --exec 'yarn serv'",
    "serv:prod": "STATIC_FOLDER=./build yarn serv",
    "prod": "yarn isomor:build && yarn build && yarn serv:prod",
    "dev": "run-screen 'yarn isomor:build:dev' 'yarn serv:dev' 'yarn start' 'tail -f test-crawler-cli.log'",
    "start": "BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "proxy": "http://127.0.0.1:3005",
  "dependencies": {
    "ansi-to-html": "^0.6.10",
    "antd": "^3.16.2",
    "brace": "^0.11.1",
    "fuse.js": "^3.4.4",
    "isomor": "^0.7.1",
    "moment": "^2.24.0",
    "moment-duration-format": "^2.2.2",
    "query-string": "^6.4.2",
    "react": "^16.8.6",
    "react-ace": "^6.4.0",
    "react-async-cache": "^0.7.0",
    "react-dom": "^16.8.6",
    "react-masonry-component": "^6.2.1",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "sharp": "^0.22.0",
    "test-crawler-cli": "^0.7.6",
    "test-crawler-lib": "^0.7.6",
    "typescript": "3.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@storybook/addon-actions": "^5.2.0-alpha.16",
    "@storybook/addon-links": "^5.2.0-alpha.16",
    "@storybook/addons": "^5.2.0-alpha.16",
    "@storybook/react": "^5.2.0-alpha.16",
    "@types/antd": "^1.0.0",
    "@types/jest": "24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/moment-duration-format": "^2.2.2",
    "@types/node": "11.13.4",
    "@types/react": "16.8.13",
    "@types/react-dom": "16.8.3",
    "@types/react-router-dom": "^4.3.1",
    "@types/sharp": "^0.22.1",
    "babel-loader": "8.0.5",
    "nodemon": "^1.18.11",
    "run-screen": "^0.5.3"
  },
  "gitHead": "af9ce9bc87080c5b936336d47f552ccccc350ac8"
}

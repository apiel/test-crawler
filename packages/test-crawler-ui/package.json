{
  "name": "test-crawler",
  "version": "0.8.0",
  "bin": {
    "test-crawler": "./test-crawler",
    "test-crawler-cli": "bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/apiel/test-crawler"
  },
  "scripts": {
    "cli-start": "PROCESS_TIMEOUT=30 test-crawler-cli",
    "cli-dev": "ts-node cli-src/index.ts",
    "cli-build": "rm -rf cli/ && tsc -p tsconfig-cli.json && chmod +x cli/index.js",
    "test-crawler": "test-crawler",
    "prepublishOnly": "cp ../../README.md . && yarn isomor:build && yarn serv:build && chmod +x test-crawler",
    "isomor:build": "NO_VALIDATION=true isomor-transpiler",
    "isomor:build:dev": "WATCH=true isomor-transpiler",
    "serv:build": "rimraf ./dist-server && tsc -p tsconfig.server.json",
    "serv": "yarn serv:build && isomor-server",
    "serv:dev": "nodemon --watch 'src-isomor/**/server/**/*' -e ts,tsx --exec 'yarn serv'",
    "serv:prod": "STATIC_FOLDER=./build yarn serv",
    "prod": "yarn isomor:build && yarn build && yarn serv:prod",
    "dev": "run-screen 'yarn isomor:build:dev' 'yarn serv:dev' 'yarn start' 'tail -f test-crawler-cli.log'",
    "start": "BROWSER=none react-scripts start",
    "app-build": "react-scripts build",
    "build": "yarn app-build && yarn cli-build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "proxy": "http://127.0.0.1:3005",
  "dependencies": {
    "ansi-to-html": "^0.6.11",
    "antd": "^3.23.2",
    "brace": "^0.11.1",
    "fuse.js": "^3.4.5",
    "isomor": "^1.6.3",
    "moment": "^2.24.0",
    "moment-duration-format": "^2.3.2",
    "query-string": "^6.8.3",
    "react": "^16.9.0",
    "react-ace": "^7.0.4",
    "react-async-cache": "^0.7.0",
    "react-dom": "^16.9.0",
    "react-masonry-component": "^6.2.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1",
    "sharp": "^0.23.0",
    "test-crawler-lib": "^0.8.0",
    "fs-extra": "^7.0.1",
    "lockfile": "^1.0.4",
    "logol": "^0.0.2",
    "minimatch": "^3.0.4",
    "pixdiff-zone": "^0.8.0",
    "pngjs": "^3.3.3",
    "puppeteer": "^1.11.0",
    "ts-node": "^8.0.2",
    "typescript": "3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "@types/antd": "^1.0.0",
    "@types/jest": "24.0.18",
    "@types/lodash": "^4.14.138",
    "@types/moment-duration-format": "^2.2.2",
    "@types/node": "12.7.4",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-router-dom": "^4.3.5",
    "@types/sharp": "^0.22.2",
    "@types/fs-extra": "^5.0.5",
    "@types/lockfile": "^1.0.1",
    "@types/minimatch": "^3.0.3",
    "@types/pngjs": "^3.3.2",
    "@types/puppeteer": "^1.11.2",
    "tslint": "^5.12.1",
    "babel-loader": "8.0.6",
    "nodemon": "^1.19.2",
    "run-screen": "^0.5.3"
  },
  "gitHead": "6221476c6a71bcdf843bb2a950781820c362de68"
}

{
  "name": "test-crawler",
  "version": "0.2.1",
  "bin": {
    "test-crawler": "./test-crawler"
  },
  "dependencies": {
    "antd": "^3.16.2",
    "brace": "^0.11.1",
    "isomor": "^0.1.8",
    "isomor-react": "^0.0.8",
    "moment": "^2.24.0",
    "moment-duration-format": "^2.2.2",
    "query-string": "^6.4.2",
    "react": "^16.8.6",
    "react-ace": "^6.4.0",
    "react-dom": "^16.8.6",
    "react-masonry-component": "^6.2.1",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "sharp": "^0.22.0",
    "test-crawler-lib": "^0.2.1",
    "typescript": "3.4.3"
  },
  "scripts": {
    "prepublishOnly": "cp ../../Readme.md . && yarn isomor:build && yarn serv:build && chmod +x test-crawler",
    "isomor:build": "isomor-transpiler",
    "isomor:build:dev": "WATCH=true isomor-transpiler",
    "serv:build": "rimraf ./dist-server && tsc -p tsconfig.server.json",
    "serv": "yarn serv:build && isomor-server",
    "serv:dev": "nodemon --watch 'src-isomor/**/server/**/*' -e ts,tsx --exec 'yarn serv'",
    "serv:prod": "STATIC_FOLDER=./build yarn serv",
    "prod": "yarn isomor:build && yarn build && yarn serv:prod",
    "dev": "run-p isomor:build:dev serv:dev start",
    "start": "BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "proxy": "http://127.0.0.1:3005",
  "devDependencies": {
    "@types/antd": "^1.0.0",
    "@types/jest": "24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/moment-duration-format": "^2.2.2",
    "@types/node": "11.13.4",
    "@types/react": "16.8.13",
    "@types/react-dom": "16.8.3",
    "@types/react-router-dom": "^4.3.1",
    "@types/sharp": "^0.22.1",
    "nodemon": "^1.18.11",
    "npm-run-all": "^4.1.5"
  },
  "gitHead": "84cce9668e844e030da59d19dbc664b6ab413cee"
}

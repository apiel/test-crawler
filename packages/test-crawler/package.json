{
  "name": "test-crawler",
  "version": "2.1.2",
  "bin": {
    "test-crawler": "./test-crawler.js",
    "test-crawler-cli": "bin/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/apiel/test-crawler"
  },
  "scripts": {
    "cli": "node bin/cli.js",
    "cli-start": "PROCESS_TIMEOUT=30 test-crawler-cli",
    "cli-dev": "ts-node --project tsconfig-cli.json cli/cli.ts",
    "cli-build": "rm -rf bin/ && tsc -p tsconfig-cli.json && chmod +x bin/cli.js",
    "test-crawler": "test-crawler",
    "prepublishOnly": "cp ../../README.md . && yarn cli-build && yarn isomor:build && yarn serv:build && chmod +x test-crawler.js",
    "isomor:build": "ISOMOR_WS=.* ISOMOR_NO_VALIDATION=true isomor-transpiler",
    "isomor:build:dev": "ISOMOR_WS=.* ISOMOR_NO_VALIDATION=true ISOMOR_WATCH=true isomor-transpiler",
    "isomor:server": "ISOMOR_WS=.* ISOMOR_NO_VALIDATION=true node ../isomor-server/dist/bin/server.js",
    "serv:build": "rimraf ./dist-server && tsc -p tsconfig.server.json",
    "serv": "yarn serv:build && isomor-server",
    "serv:dev": "nodemon --watch 'src-isomor/**/server/**/*' -e ts,tsx --exec 'yarn serv'",
    "serv:prod": "ISOMOR_STATIC_FOLDER=./build yarn serv",
    "prod": "yarn isomor:build && yarn build && yarn serv:prod",
    "dev": "ISOMOR_WS=.* run-screen 'yarn isomor:build:dev' 'yarn serv:dev' 'yarn start' 'tail -f test-crawler-cli.log'",
    "start": "BROWSER=none react-scripts start",
    "app-build": "react-scripts build",
    "build": "yarn app-build && yarn cli-build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "github:build:static": "rm -rf src && cp -r src-isomor src && rm src/server/lib/CrawlerProviderStorage.ts && mv src/server/lib/CrawlerProviderGitHubStorage.ts src/server/lib/CrawlerProviderStorage.ts && rm src/server/storage.typing.ts && mv src/server/storageGitHub.typing.ts src/server/storage.typing.ts && rm -rf src/server/lib/crawl  && rm -rf src/server/lib/diff && rm src/server/lib/storage/LocalStorage.ts",
    "github:pub": "yarn github:build:static && PUBLIC_URL=/test-crawler/live yarn build && cp -r build/* ../../docs/live && yarn build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "proxy": "http://127.0.0.1:3005",
  "dependencies": {
    "ansi-to-html": "^0.6.14",
    "antd": "^3.26.9",
    "axios": "^0.19.2",
    "brace": "^0.11.1",
    "expect": "^25.1.0",
    "fs-extra": "^8.1.0",
    "fuse.js": "^3.4.6",
    "isomor": "^2.1.1",
    "lockfile": "^1.0.4",
    "logol": "^0.0.3",
    "md5": "^2.2.1",
    "minimatch": "^3.0.4",
    "moment": "^2.24.0",
    "moment-duration-format": "^2.3.2",
    "pixdiff-zone": "^1.0.0-alpha.0",
    "pngjs": "^3.3.3",
    "puppeteer": "^2.1.1",
    "query-string": "^6.11.0",
    "react": "^16.12.0",
    "react-ace": "^8.0.0",
    "react-async-cache": "^1.1.0",
    "react-dom": "^16.12.0",
    "react-hotjar": "^2.2.0",
    "react-masonry-component": "^6.2.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.1",
    "rimraf": "^3.0.2",
    "sharp": "^0.24.0",
    "ts-node": "^8.6.2",
    "typescript": "3.7.5",
    "universal-cookie": "^4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@storybook/addon-actions": "^5.3.13",
    "@storybook/addon-links": "^5.3.13",
    "@storybook/addons": "^5.3.13",
    "@storybook/react": "^5.3.13",
    "@types/antd": "^1.0.0",
    "@types/axios": "^0.14.0",
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "25.1.2",
    "@types/lockfile": "^1.0.1",
    "@types/lodash": "^4.14.149",
    "@types/md5": "^2.1.33",
    "@types/minimatch": "^3.0.3",
    "@types/moment-duration-format": "^2.2.2",
    "@types/node": "13.7.1",
    "@types/pngjs": "^3.4.1",
    "@types/puppeteer": "^2.0.0",
    "@types/react": "16.9.19",
    "@types/react-dom": "16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/rimraf": "^2.0.3",
    "@types/sharp": "^0.24.0",
    "babel-loader": "8.0.6",
    "nodemon": "^2.0.2",
    "run-screen": "^0.5.3",
    "tslint": "^6.0.0"
  },
  "gitHead": "38f02579dceba5261ae78137c1adefca34328abd"
}

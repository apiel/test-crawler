{
  "name": "test-crawler-cli",
  "version": "3.2.1",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "test-crawler-cli": "cli/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/apiel/test-crawler"
  },
  "scripts": {
    "cli": "node cli/cli.js",
    "cli-dev": "ts-node --project tsconfig-cli.json src/cli/cli.ts",
    "rm": "rm -rf cli/ && rm -rf lib/",
    "move": "mv dist/* . && rm -rf dist/",
    "build": "yarn rm && tsc -p tsconfig.json && yarn move && chmod +x cli/cli.js",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "chromedriver": "^80.0.1",
    "fs-extra": "^8.1.0",
    "geckodriver": "^1.19.1",
    "logol": "^0.0.3",
    "md5": "^2.2.1",
    "minimatch": "^3.0.4",
    "pixdiff-zone": "^3.0.1",
    "pngjs": "^3.3.3",
    "puppeteer": "^2.1.1",
    "rimraf": "^3.0.2",
    "selenium-webdriver": "^4.0.0-alpha.5",
    "test-crawler-core": "^3.2.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/fs-extra": "^8.0.1",
    "@types/md5": "^2.1.33",
    "@types/minimatch": "^3.0.3",
    "@types/node": "13.7.4",
    "@types/pngjs": "^3.4.1",
    "@types/puppeteer": "^2.0.0",
    "@types/rimraf": "^2.0.3",
    "@types/selenium-webdriver": "^4.0.8",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "typescript": "3.8.2"
  }
}
